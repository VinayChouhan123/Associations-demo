<h1>Edit Appointment</h1>

<%= form_for @appointment do |form| %> 
	<div>
		<%= form.label :docname %><br>
		<%= form.text_field :docname %>
	</div><br>

	<div>
		<%= form.label :address %><br>
		<%= form.text_area :address %>
	</div><br>

	<div>
		<%= form.label :'patient id' %><br>
		<%= form.number_field :patient_id %>
	</div><br>

	<div>
		<%= form.label :'physician id' %><br>
		<%= form.number_field :physician_id %>
	</div><br>
	<%= form.submit %>

<% end %>
